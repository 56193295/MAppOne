<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mnls.com.mappone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

       <!-- Activity的信息注册-->
        <activity
            android:name=".RegistActivity"
            android:label="@string/title_activity_regist"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainFragment"
            android:label="@string/title_activity_frag"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ModifyMe"
            android:label="@string/title_activity_modity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ResetActivity"
            android:label="@string/title_activity_reset"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ForgetActivity"
            android:label="@string/title_activity_forget"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OrderSubmit"
            android:label="@string/title_activity_submit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CheckActivity"
            android:label="@string/title_activity_check"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TimeActivity"
            android:label="@string/title_activity_time"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>